<entity-mappings xmlns="http://www.hibernate.org/xsd/orm/mapping"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 version="3.2">
    <entity class="org.hibernate.models.orm.xml.attr.ManyToOneTests$SimpleEntity" metadata-complete="true" access="FIELD">
        <attributes>
            <id name="id"/>
            <basic name="name"/>
            <many-to-one name="parent">
                <join-column name="parent_fk" insertable="false" updatable="false"/>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>